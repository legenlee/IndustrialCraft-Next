{
	"schemaVersion": 1,
	"id": "icnext",
	"version": "${version}",
	"name": "IndustrialCraft-Next",
	"description": "Old things to next generation. IndustrialCraft-Next.",
	"authors": [
		"LegenLee"
	],
	"contact": {
		"homepage": "https://github.com/legenlee/industrialcraft-next",
		"sources": "https://github.com/legenlee/industrialcraft-next"
	},
	"license": "CC0-1.0",
	"icon": "assets/icnext/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"legenlee.industrialcraft.TemplateMod"
		],
		"client": [
			"legenlee.industrialcraft.TemplateModClient"
		],
		"fabric-datagen": [
			"legenlee.industrialcraft.TemplateModDataGenerator"
		]
	},
	"mixins": [
		"icnext.mixins.json",
		{
			"config": "icnext.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}